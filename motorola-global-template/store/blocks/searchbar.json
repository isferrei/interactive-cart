{
    "search-bar": {
        "blocks": [
            "autocomplete-result-list.v2"
        ],
        "props": {
            "openAutocompleteOnFocus": true,
            "containerMode": "container",
            "simulationBehavior": "default"
        }
    },
    "autocomplete-result-list.v2": {
        "blocks": [
            "product-summary.shelf#autocomplete"
        ]
    },
    "product-summary.shelf#autocomplete": {
        "children": [
            "flex-layout.row#shelf-autocomplete"
        ],
        "props": {
            "blockClass": "shelf-autocomplete"
        }
    },
    "flex-layout.row#shelf-autocomplete": {
        "children": [
            "flex-layout.col#shelf-autocomplete-col1",
            "flex-layout.col#shelf-autocomplete-col2"
        ],
        "props": {
            "blockClass": "autocomplete-row"
        }
    },
    "flex-layout.col#shelf-autocomplete-col1": {
        "children": [
            "product-summary-image#shelf"
        ],
        "props": {
            "blockClass": "autocomplete-col1"
        }
    },
    "flex-layout.col#shelf-autocomplete-col2": {
        "children": [
            "product-summary-name",
            "product-selling-price"
        ],
        "props": {
            "blockClass": "autocomplete-col2"
        }
    },
    "product-summary-image#shelf": {
        "props": {
            "showBadge": false,
            "aspectRatio": "1:1",
            "maxHeight": 300,
            "blockClass": "shelf"
        }
    }
}